
<div class="container">
    <h1>{{profile.username}}</h1>
    {{#each profile.posts}}
    <div class="card mt-4">
        <div class="card-body">
            <h3 class="card-title">{{this.title}}</h3>
            <div class="card-text mb-2">
            <p>{{this.content}}</p>
            <p>{{this.likes}} likes, {{this.comments.length}} comments</p>
            </div>
        </div>
    </div>
    {{/each}}
</div>
